{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:bpmn-io/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": [ "warn", { "ignoreRestArgs": true } ]
  },
  "globals": {
    "AggregateError": false
  }
}